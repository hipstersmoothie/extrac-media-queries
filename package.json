{
  "name": "prepend-media-queries",
  "version": "1.0.0",
  "description": "Strips out all media queries based on user provided breakpoints and prepends all effected classes with a breakpoint specific wrapper.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "webpack-loader",
    "webpack",
    "media",
    "@media",
    "media-query"
  ],
  "scripts": {
    "clean": "rm -rf lib/",
    "prepublish": "npm run clean && node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "lint": "eslint src/ ",    
    "test": "ava --tap | tnyan",
    "test:watch": "ava --watch"
  },
  "main": "lib/transform.js",
  "author": "Andrew Lisowski",
  "homepage": "https://github.com/hipstersmoothie/prepend-media-queries",
  "repository": "",
  "bugs": {
    "url": "https://github.com/hipstersmoothie/prepend-media-queries/issues"
  },
  "license": "MIT",
  "dependencies": {
    "loader-utils": "^1.1.0",
    "postcss": "^6.0.14"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "tap-nyan": "^1.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "ava": {
    "require": "babel-core/register",
    "babel": "inherit"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "indent": ["error", 4],
      "semi": ["error", "never"],
      "no-param-reassign": 0
    }
  }
}
